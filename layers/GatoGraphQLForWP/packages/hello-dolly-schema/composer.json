{
    "name": "dolly-shepherd/hello-dolly-schema",
    "type": "library",
    "description": "GitHub template repository to quickstart your extension for Gato GraphQL.",
    "authors": [
        {
            "name": "My Company",
            "email": "name@company.com",
            "homepage": "https://mysite.com"
        }
    ],
    "keywords": ["WordPress", "Gato", "GatoGraphQL", "GraphQL", "HelloDolly"],
    "homepage": "https://github.com/MyCompanyForGatoGraphQL/HelloDollySchema",
    "require": {
        "php": "^8.1",
        "pop-schema/schema-commons": "^1.1"
    },
    "require-dev": {
        "johnpbloch/wordpress": "^6.3",
        "phpunit/phpunit": "^10.2",
        "rector/rector": "^0.18",
        "squizlabs/php_codesniffer": "^3.6",
        "szepeviktor/phpstan-wordpress": "^1.0",
        "wpackagist-plugin/hello-dolly": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "MyCompanyForGatoGraphQL\\HelloDollySchema\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyCompanyForGatoGraphQL\\HelloDollySchema\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests",
        "analyse": "phpstan analyse"
    },
    "extra": {
        "wordpress-install-dir": "vendor/wordpress/wordpress",
        "branch-alias": {
            "dev-main": "1.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
