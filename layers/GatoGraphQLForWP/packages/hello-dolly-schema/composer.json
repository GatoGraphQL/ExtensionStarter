{
    "name": "my-company-for-gatographql/hello-dolly-schema",
    "type": "library",
    "description": "Schema for the integration of plugin Hello Dolly with Gato GraphQL",
    "authors": [
        {
            "name": "My Company",
            "email": "name@mycompany.com",
            "homepage": "https://mycompany.com"
        }
    ],
    "keywords": ["WordPress", "plugin", "Gato", "GatoGraphQL", "GraphQL", "Extension", "Starter"],
    "homepage": "https://github.com/GatoGraphQL/hello-dolly-schema",
    "require": {
        "php": "^8.1",
        "pop-schema/schema-commons": "^1.1"
    },
    "require-dev": {
        "johnpbloch/wordpress": "^6.4",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^10.2",
        "rector/rector": "^0.18",
        "squizlabs/php_codesniffer": "^3.6",
        "szepeviktor/phpstan-wordpress": "^1.0",
        "wp-coding-standards/wpcs": "^3.0",
        "wpackagist-plugin/hello-dolly": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "MyCompanyForGatoGraphQL\\HelloDollySchema\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyCompanyForGatoGraphQL\\HelloDollySchema\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests",
        "analyse": "phpstan analyse"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "wordpress-install-dir": "vendor/wordpress/wordpress",
        "branch-alias": {
            "dev-main": "1.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
