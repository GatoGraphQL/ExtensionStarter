{
    "name": "extension-vendor/extension-name-schema",
    "type": "library",
    "description": "GitHub template repository to quickstart your extension for Gato GraphQL.",
    "keywords": ["Gato", "GatoGraphQL", "GraphQL", "PHP", "WordPress"],
    "homepage": "https://github.com/ExtensionVendor/ExtensionNameSchema",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "^8.1",
        "pop-cms-schema/schema-commons": "^1.1"
    },
    "require-dev": {
        "johnpbloch/wordpress": "^6.3",
        "phpunit/phpunit": "^10.2",
        "rector/rector": "^0.18",
        "squizlabs/php_codesniffer": "^3.6",
        "szepeviktor/phpstan-wordpress": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ExtensionVendor\\ExtensionNameSchema\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExtensionVendor\\ExtensionNameSchema\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests",
        "analyse": "phpstan analyse"
    },
    "extra": {
        "wordpress-install-dir": "vendor/wordpress/wordpress",
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
